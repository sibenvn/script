local hash = "{{hash}}"
local Gui = loadstring(game:HttpGet(("https://raw.githubusercontent.com/sibenvn/script/refs/heads/main/theme/wizard"), true))()
local Window = Gui:NewWindow("游 MENU SIBEN 游")
local isLoading = false

--[[ Defined ]]--
local StarterGui = cloneref(game:GetService("StarterGui"))

--[[ Notification ]]--
local function alert(t) StarterGui:SetCore("SendNotification", { Title = "Th칪ng b치o", Text = t, Duration = 2 }) end

--[[ Script Section ]]--
local ScriptSection = Window:NewSection("Script")

function loadscript(url)
    if isLoading == false then
        isLoading = true
        alert("캟ang th敲쎢 thi script...")
        local success, errorMessage = pcall(function()
            loadstring(game:HttpGet((url), true))()
        end)
        if success then
            alert("캟칚 th敲쎢 thi ch故몇 script")
        else
            alert("Script 캠칚 x故믗 ra l敲들")
            print("캟칚 x故믗 ra l敲들: " .. errorMessage)
        end
        isLoading = false
    end
end

ScriptSection:CreateButton("Console", function()
    StarterGui:SetCore("DevConsoleVisible", true)
end)

ScriptSection:CreateDropdown("Select", {
    "get_pos",
    "fix_lag",
    "test"
}, 2, function(text)
    selected = text
end)

ScriptSection:CreateButton("Excute", function()
    if selected == "fix_lag" then
        loadscript("https://raw.githubusercontent.com/sibenvn/script/" .. hash .. "/siben")
    elseif selected == "get_pos" then
        loadscript("https://raw.githubusercontent.com/sibenvn/script/" .. hash .. "/siben")
    else
        loadscript("https://raw.githubusercontent.com/sibenvn/script/" .. hash .. "/siben")
    end
end)